<h2>Delete Expense?</h2>

<form>
    <div class="form-group">
        <label>Amount: <input type="text"  class="form-control" ng-model="Amount" readonly></label>
    </div>
    <div class="form-group">
        <label>Date: <input type="date" class="form-control" ng-model="Date" readonly></label>
    </div>
    <button type="submit" class="btn btn-primary" ng-click="Delete()">Delete</button>
    <div ng-class="resultCss">{{message}}</div>
</form>